<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Registtration</servlet-name>
        <servlet-class>Upload.Registtration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tweets</servlet-name>
        <servlet-class>Upload.Tweets</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registtration</servlet-name>
        <url-pattern>/Registtration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tweets</servlet-name>
        <url-pattern>/Tweets</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
